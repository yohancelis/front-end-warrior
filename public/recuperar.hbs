<body>
  <div>
    <img src="./build/images/img.png" id="imgLog1">
    <img src="./build/images/logo-login-white.png" id="imgLog2">
  </div>
  <form class="container formulario">
    <h3 class="center">Recuperación de contraseña</h3>
    <div class="center">
      <div class="input-field">
        <i class="material-icons prefix">email</i>
        <input type="email" id="email" class="" />
        <label for="email">Correo</label>
        <span class="helper-text">ejemplo@gmail.com</span>
      </div>
    </div>
    <div id="alerta" class="center"></div><br />
    <div class="center">
      <button type="button" class="waves-effect waves-perso btn black">Ingresar</button>
      <button type="button" class="waves-effect waves-perso btn black"><a href="/">Cancelar</a></button>
    </div>
  </form>
  <script src="build/js/login.js"></script>
</body>

